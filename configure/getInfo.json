{
    "URL":"https://masiro.me/admin/novelView",
    "isSaveCookie":true,
    "isWithCookie":true,
    "isWithData":true,
    "data":{
        "method":"params",
        "dataset":[
            {
                "name":"novel_id",
                "type":"int",
                "required":true
            }
        ]
    },
    "response":{
        "order":[
            "title","brief","ori",
            "author","translator",
            "status","tags",
            "comment","like","mark","read",
            "temp_mark_status","temp2_mark_status","mark_status","read_status"
        ],
        "title":{
            "type":"regex",
            "func":"hunt",
            "rule":"(?<=\"novel-title\">).+?(?=</div>)"
        },
        "brief":{
            "type":"regex",
            "func":"hunt",
            "rule":"(?<=\"brief\">)[\\s\\S\\n]+?(?=</div>)"
        },
        "ori":{
            "type":"regex",
            "func":"hunt",
            "rule":"(?<=原作 : )[\\s\\S\\n]+?(?=</div>)"
        },
        "author":{
            "type":"regex",
            "func":"hunt",
            "rule":"(?<=author=).+?(?=\")"
        },
        "translator":{
            "type":"regex",
            "func":"find",
            "rule":"(?<=translator=).+?(?=\")"
        },
        "status":{
            "type":"regex",
            "func":"hunt",
            "rule":"(连载中|完结)"
        },
        "tags":{
            "type":"regex",
            "func":"find",
            "rule":"(?<=标签:).+?(?=&)"
        },
        "comment":{
            "type":"regex",
            "func":"hunt",
            "rule":"\\d+?(?=评论)"
        },
        "like":{
            "type":"regex",
            "func":"hunt",
            "rule":"\\d+?(?=点赞)"
        },
        "mark":{
            "type":"regex",
            "func":"hunt",
            "rule":"\\d+?(?=收藏)"
        },
        "read":{
            "type":"regex",
            "func":"hunt",
            "rule":"\\d+?(?=阅读)"
        },
        "temp_mark_status":{
            "type":"regex",
            "func":"seek",
            "rule":"<span\\s+class=\"[^\"]+\"\\s+(?!style=\"display:\\s*none;\")[^>]*>(.*?)</span>"
        },
        "temp2_mark_status":{
            "type":"range",
            "refer":"temp_mark_status",
            "rule":{
                "order":["gets","return"],
                "gets":{
                    "type":"regex",
                    "func":"hunt",
                    "rule":"已?收藏"
                },
                "return":{
                    "type":"return",
                    "refer":"gets"
                }
            }
        },
        "mark_status":{
            "type":"refer",
            "rule":["temp2_mark_status", 0]
        },
        "read_status":{
            "type":"regex",
            "func":"hunt",
            "rule":"(?<=\")\\d+(?=\">继续阅读)"
        }
    }
}
