{
    "URL":"https://masiro.me/admin/forumList",
    "isSaveCookie":true,
    "isWithCookie":true,
    "isWithData":true,
    "data":{
        "method":"params",
        "dataset":[
            {
                "name":"type",
                "type":"int",
                "required":false
            },
            {
                "name":"page",
                "type":"int",
                "required":false
            },
            {
                "name":"keyword",
                "type":"str",
                "required":false
            }
        ]
    },
    "response":{
        "order":[
            "temp_forum","forums","return"
        ],
        "temp_forum":{
            "type":"regex",
            "func":"seek",
            "rule":"<a href=\"/ad[\\s\\S\\n]+?</a>"
        },
        "forums":{
            "type":"range",
            "refer":"temp_forum",
            "rule":{
                "order":["url","image","name","brief"],
                "url":{
                    "type":"regex",
                    "func":"hunt",
                    "rule":"(?<=href=\").+?(?=\")"
                },
                "image":{
                    "type":"regex",
                    "func":"hunt",
                    "rule":"(?<=src=\").+?(?=\")"
                },
                "name":{
                    "type":"regex",
                    "func":"hunt",
                    "rule":"(?<=name\">).+?(?=<)"
                },
                "brief":{
                    "type":"regex",
                    "func":"hunt",
                    "rule":"(?<=brief\">)[\\s\\S\\n]+?(?=<)"
                }
            }
        },
        "return":{
            "type":"return",
            "refer":"forums"
        }
    }
}
