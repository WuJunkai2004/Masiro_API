{
    "URL":"https://masiro.me/admin/loadMoreNovels",
    "isSaveCookie":true,
    "isWithCookie":true,
    "isWithData":true,
    "data":{
        "method":"params",
        "dataset":[
            {
                "name":"keyword",
                "type":"str",
                "required":true
            },
            {
                "name":"page",
                "type":"int",
                "required":false
            },
            {
                "name":"ori",
                "type":"int",
                "required":false
            },
            {
                "name":"tags",
                "type":"list",
                "required":false
            },
            {
                "name":"tags_inverse",
                "type":"list",
                "required":false
            },
            {
                "name":"order",
                "type":"int",
                "required":false
            }
        ]
    },
    "response":{
        "order":[
            "decode","find"
        ],
        "decode":{
            "type":"func",
            "func":"code"
        },
        "find":{
            "type":"find",
            "rule":"<a ([\\s\\S\n]+?<\\\\/div>){8}"
        }
    }
}
