{
    "URL":"https://masiro.me/admin/loadMoreNovels",
    "isSaveCookie":true,
    "isWithCookie":true,
    "isWithData":true,
    "data":{
        "method":"params",
        "dataset":[
            {
                "name":"keyword",
                "type":"str",
                "required":true
            },
            {
                "name":"page",
                "type":"int",
                "required":false
            },
            {
                "name":"ori",
                "type":"int",
                "required":false
            },
            {
                "name":"tags",
                "type":"list",
                "required":false
            },
            {
                "name":"tags_inverse",
                "type":"list",
                "required":false
            },
            {
                "name":"order",
                "type":"int",
                "required":false
            }
        ]
    },
    "response":{
        "order":[
            "decode","find","detail"
        ],
        "decode":{
            "type":"func",
            "func":"code"
        },
        "find":{
            "type":"find",
            "rule":"<a ([\\s\\S\n]+?<\\\\/div>){8}"
        },
        "detail":{
            "type":"range",
            "refer":"find",
            "rule":{
                "order":[
                    "title","novel_id","author"
                ],
                "title":{
                    "type":"regex",
                    "rule":"(?<=title=\").+?(?=\")"
                },
                "novel_id":{
                    "type":"regex",
                    "rule":"(?<=novel_id=)\\d+?(?=\")"
                },
                "author":{
                    "type":"regex",
                    "rule":"(?<=作者: ).+?(?=<)"
                }
            }
        }
    }
}
