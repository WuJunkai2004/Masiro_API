{
    "URL":"https://masiro.me/admin/novelView",
    "isSaveCookie":true,
    "isWithCookie":true,
    "isWithData":true,
    "data":{
        "method":"params",
        "dataset":[
            {
                "name":"novel_id",
                "type":"int",
                "required":true
            }
        ]
    },
    "response":{
        "order":[
            "temp_chapter","charpers","return"
        ],
        "temp_chapter":{
            "type":"regex",
            "func":"find",
            "rule":"\"chapter-box\"([\\s\\S\\n]+?(<ul|ul>)){2}"
        },
        "charpers":{
            "type":"range",
            "refer":"temp_chapter",
            "rule":{
                "order":["charpter_name","temp_article","articles"],
                "charpter_name":{
                    "type":"regex",
                    "func":"hunt",
                    "rule":"(?<=<b>).+?(?=&)"
                },
                "temp_article":{
                    "type":"regex",
                    "func":"find",
                    "rule":"<a[\\s\\S\\n]+?li>"
                },
                "articles":{
                    "type":"range",
                    "refer":"temp_article",
                    "rule":{ 
                        "order":["title","url","latest"],
                        "title":{
                            "type":"regex",
                            "func":"hunt",
                            "rule":".+?(?=&nbsp;)"
                        },
                        "url":{
                            "type":"regex",
                            "func":"hunt",
                            "rule":"(?<=href=\").+?(?=\")"
                        },
                        "latest":{
                            "type":"regex",
                            "func":"hunt",
                            "rule":"\\d\\d-\\d\\d-\\d\\d\\s\\d\\d:\\d\\d"
                        }
                    }
                }
            }
        },
        "return":{
            "type":"return",
            "refer":"charpers"
        }
    }
}
